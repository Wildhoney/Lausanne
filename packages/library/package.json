{
  "name": "lausanne",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.tsx",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "types": "./dist/index.server.d.ts",
      "default": "./dist/index.server.js"
    },
    "default": {
      "types": "./dist/index.client.d.ts",
      "default": "./dist/index.client.js"
    }
  },
  "scripts": {
    "build": "tsc && webpack && rm -rf dist/client",
    "format": "prettier . --write",
    "unit": "jest",
    "lint": "eslint src/** --fix",
    "typecheck": "tsc --noEmit"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [],
        "dependsOn": [
          "^build"
        ]
      },
      "typecheck": {
        "inputs": [
          "default"
        ],
        "outputs": [],
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "dependencies": {
    "preact": "10.13.0",
    "preact-render-to-string": "5.2.6"
  },
  "devDependencies": {
    "jest-environment-jsdom": "29.4.3",
    "ts-loader": "9.4.2",
    "webpack": "5.76.1",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  }
}
