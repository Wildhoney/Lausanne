      },
      "typecheck": {
        "inputs": [
          "default"
        ],
        "outputs": [],
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "dependencies": {
    "preact": "10.13.0",
    "preact-render-to-string": "5.2.6"
  },
  "devDependencies": {
    "jest-environment-jsdom": "29.4.3"
  }
