{
  "name": "lausanne",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.tsx",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "types": "./dist/index.server.d.ts",
      "default": "./dist/index.server.js"
    },
    "default": {
      "types": "./dist/index.client.d.ts",
      "default": "./dist/index.client.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier . --write",
    "unit": "jest",
    "lint": "eslint src/** --fix",
    "typecheck": "tsc --noEmit"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [],
        "dependsOn": [
          "^build"
